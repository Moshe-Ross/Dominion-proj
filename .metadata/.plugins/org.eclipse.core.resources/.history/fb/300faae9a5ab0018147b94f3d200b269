package dominion;

public class Victory extends Card{
	
	public void estate(){
		super.cost = 2;
		victoryPoint = 1;
		
//		private File srcImg = new File("/dominion/src/dominion.images/estate.png");
//		private Image img = new ImageIO.read(srcImg);	
	}

	public void duchy(){
		super.cost = 5;
		victoryPoint = 3;
		
//		private File srcImg = new File("/dominion/src/dominion.images/duchy.png");
//		private Image img = new ImageIO.read(srcImg);	
	}
	
	public void province(){
		super.cost = 8;
		victoryPoint = 6;
		
//		private File srcImg = new File("/dominion/src/dominion.images/province.png");
//		private Image img = new ImageIO.read(srcImg);	
	}
	
	public int getVictoryPoint(){
		return victoryPoint;
	}
}
